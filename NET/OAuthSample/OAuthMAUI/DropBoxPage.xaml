<?xml version="1.0" encoding="utf-8"?>

<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="OAuthMAUI.DropBoxPage">
	<ContentPage.Content>
		<VerticalStackLayout>
			<Label Text="DropBox" />
			<HorizontalStackLayout>
				<Button x:Name="Login" Text="Login"
				        Clicked="Login_OnClicked" />
				<Button x:Name="LogOut" Text="Logout"
				        Clicked="LogOut_OnClicked" />
			</HorizontalStackLayout>
			<Label Text="Status:" />
		</VerticalStackLayout>


	</ContentPage.Content>
</ContentPage>